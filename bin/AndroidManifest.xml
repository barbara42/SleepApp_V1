<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sleepapp_v1"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="18" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.sleepapp_v1.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.sleepapp_v1.PSQIsurvey"
            android:label="@string/title_activity_psqisurvey" >
        </activity>
        <activity
            android:name="com.example.sleepapp_v1.PSQIsurvey_fragment1"
            android:label="@string/title_activity_psqisurvey_fragment1" >
        </activity>
        <activity
            android:name="com.example.sleepapp_v1.PSQIsurvey_fragment2"
            android:label="@string/title_activity_psqisurvey_fragment2" >
        </activity>
        <activity
            android:name="com.example.sleepapp_v1.PSQIsurvey_fragment3"
            android:label="@string/title_activity_psqisurvey_fragment3" >
        </activity>
        <activity
            android:name="com.example.sleepapp_v1.TimePickerFragment"
            android:label="@string/title_activity_time_picker_fragment" >
        </activity>
        <activity
            android:name="com.example.sleepapp_v1.SleepSensorActivity"
            android:label="@string/title_activity_sleep_sensor" >
        </activity>
        <activity
            android:name="com.example.sleepapp_v1.DataView"
            android:label="@string/title_activity_data_view" >
        </activity>
    </application>

</manifest>
